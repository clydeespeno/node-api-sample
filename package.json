{
  "name": "webapp",
  "version": "0.0.1",
  "description": "Simple node api app",
  "main": "dist/app.js",
  "engines": {
    "node": ">=6.7.0"
  },
  "dependencies": {
    "babel-cli": "6.14.0",
    "babel-core": "6.14.0",
    "babel-eslint": "6.1.2",
    "babel-loader": "6.2.5",
    "babel-polyfill": "6.13.0",
    "babel-preset-es2015": "6.5.0",
    "babel-preset-stage-0": "6.5.0",
    "babel-register": "6.14.0",
    "babel-runtime": "6.11.6",
    "babel-watch": "2.0.3-rc0 ",
    "bluebird": "3.4.6",
    "body-parser": "1.15.2",
    "eslint": "3.4.0",
    "eslint-plugin-promise": "1.0.8",
    "express": "4.14.0",
    "lodash": "4.16.2",
    "mkdirp": "0.5.1",
    "moment": "2.15.1",
    "nconf": "0.8.4",
    "node-fetch": "1.6.3",
    "winston": "2.2.0",
    "winston-daily-rotate-file": "1.2.0"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "isparta-loader": "2.0.0",
    "karma": "0.13.22",
    "karma-chai": "0.1.0",
    "karma-coverage": "0.5.5",
    "karma-mocha": "0.2.1",
    "karma-phantomjs-launcher": "1.0.0",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "1.7.0",
    "mocha": "2.5.3",
    "phantomjs-prebuilt": "2.1.7",
    "pm2": "2.0.18",
    "sinon": "2.0.0-pre.2",
    "sinon-chai": "2.8.0",
    "webpack": "1.12.12"
  },
  "scripts": {
    "lint": "eslint -c .eslintrc ./**/*.js",
    "build": "babel src -d dist",
    "watch": "npm run lint && babel-watch src/app.js --watch ./configuration",
    "start": "npm run lint && npm run build && node dist/app.js",
    "test": "./node_modules/karma/bin/karma start ./karma.conf.js --single-run=false",
    "test:single": "./node_modules/karma/bin/karma start ./karma.conf.js --single-run=true"
  }
}
